while i < 13  : 
    if i % 2 != 0 :
        A = A + int(id[i-1])
        i = i + 1
    elif i % 2 == 0 :
        B = B + int(id[i-1])
        i = i + 1
D = A + 3*B
if D % 10 != 0 :
    F = 10 - D%10
else :
    F = 0  
if F == int(id[-1]) :
    kq = 'Là mã vạch đúng'
else :
    kq = 'Không phải mã vạch'
print(kq)
