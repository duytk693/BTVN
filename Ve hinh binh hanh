import turtle
x1 = float(input())
y1 = float(input())
x2 = float(input())
y2 = float(input())
x3 = float(input())
y3 = float(input())
x4 = float(x3 - (x2 - x1))
y4 = float(y3 - (y2 - y1))

t = turtle.Turtle()
t.shape('turtle')
t.penup()
t.setpos(x1,y1)
t.pendown()
t.goto(x2,y2)
t.goto(x3,y3)
t.goto(x4,y4)
t.goto(x1,y1)
t.hideturtle
turtle.done()
